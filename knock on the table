#include<algorithm>
#include<iostream>
#include<ctime>
using namespace std;
int main()
{
    int i;
    for(i=1;i<=100;i++)
    {
        
        if (i%7==0||i%10==7||i/10==7)
        {
            cout<<"knock on the table"<<endl;
        }
        else
        {
            cout<<i<<endl;
        }
        
    }
    system("pause");
    return 0;
}
//knock on the table if you encounter 7 from 1-100
