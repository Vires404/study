#include<algorithm>
#include<iostream>
#include<ctime>
using namespace std;
int main()
{
    for(int i=1;i<=9;i++)
    {
        
        cout<<endl;
        for(int j=1;j<=i;j++)
        {
            cout<<j<<"*"<<i<<"="<<j*i<<" ";
        }
        cout<<endl;
    }
    system("pause");
    return 0;
}
//produce a multiplication table
